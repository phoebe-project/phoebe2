[phoebe--component--myprimary]
atm        = kurucz     #  Bolometric Atmosphere model
alb        = 1.0        #  Bolometric albedo (alb heating, 1-alb reflected)
redist     = 0.0        #  Global redist par (1-redist) local heating, redist global heating
syncpar    = 1.54186025045 #  Synchronicity parameter
gravb      = 1.0        #  Bolometric gravity brightening
pot        = 4.7        #  Roche potential value
teff       = 21700.0    # (K) Mean effective temperature
irradiator = True       #  Treat body as irradiator of other objects
abun       = 0.0        #  Metallicity
label      = myprimary  #  Name of the body
ld_func    = claret     #  Bolometric limb darkening model
ld_coeffs  = kurucz     #  Bolometric limb darkening coefficients


[phoebe--component--mysecondary]
atm        = kurucz     #  Bolometric Atmosphere model
alb        = 1.0        #  Bolometric albedo (alb heating, 1-alb reflected)
redist     = 0.0        #  Global redist par (1-redist) local heating, redist global heating
syncpar    = 2.50197843753 #  Synchronicity parameter
gravb      = 1.0        #  Bolometric gravity brightening
pot        = 10.6023510041 #  Roche potential value
teff       = 22400.0    # (K) Mean effective temperature
irradiator = True       #  Treat body as irradiator of other objects
abun       = 0.0        #  Metallicity
label      = mysecondary #  Name of the body
ld_func    = claret     #  Bolometric limb darkening model
ld_coeffs  = kurucz     #  Bolometric limb darkening coefficients


[phoebe--orbit--V380Cyg]
dpdt       = 0.0        # (s/yr) Period change
dperdt     = 0.0        # (deg/yr) Periastron change
ecc        = 0.0        #  Eccentricity
t0         = 2454000.0  # (JD) Zeropoint date
incl       = 78.7835910017 # (deg) Inclination angle
label      = V380Cyg    #  Name of the system
period     = 12.425719  # (d) Period of the system
per0       = 141.48     # (deg) Periastron
phshift    = 0.0        #  Phase shift
q          = 0.609661016949 #  Mass ratio
sma        = 60.2528369131 # (Rsol) Semi major axis
long_an    = 0.0        # (deg) Longitude of ascending node
c1label    = myprimary  #  ParameterSet connected to the primary component
c2label    = mysecondary #  ParameterSet connected to the secondary component

sma1 ~  {sma} / (1.0 + 1.0/{q})
sma2 ~  {sma} / (1.0 + {q})
totalmass ~  4*pi**2 * {sma}**3 / {period}**2 / constants.GG
mass1 ~  4*pi**2 * {sma}**3 / {period}**2 / constants.GG / (1.0 + {q})
mass2 ~  4*pi**2 * {sma}**3 / {period}**2 / constants.GG / (1.0 + 1.0/{q})
asini ~  {sma} * sin({incl})
com ~  {q}/(1.0+{q})*{sma}
q1 ~  {q}
q2 ~  1.0/{q}


[phoebe--globals--V380Cyg_globals]
ra         = 0.0        # (deg) Right ascension
dec        = 0.0        # (deg) Declination
epoch      = J2000      #  Epoch of coordinates
pmra       = 0.0        # (mas/yr) Proper motion in right ascension
pmdec      = 0.0        # (mas/yr) Proper motion in declination
distance   = 10.0        # (pc) Distance to the object
vgamma     = 20.0       # (km/s) Systemic velocity



