<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-phoebe">
    <comment>PHOEBE parameter file</comment>
    <magic priority="80">
      <match value="# Parameter file conforming to PHOEBE" type="string" offset="0"/>
    </magic>
    <glob pattern="*.phoebe"/>
